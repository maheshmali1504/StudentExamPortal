<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>AddSrevlet</servlet-name>
		<display-name>AddSrevlet</display-name>
		<description></description>
		<servlet-class>com.servlet.AddSrevlet</servlet-class>
	</servlet>




	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>com.connection.LoginServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>AddSrevlet</servlet-name>
		<url-pattern>/AddSrevlet</url-pattern>
	</servlet-mapping>




	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>



</web-app>
